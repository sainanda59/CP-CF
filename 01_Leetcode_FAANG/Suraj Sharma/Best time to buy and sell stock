class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int maxP=0;
        int n=prices.size();
        int mini=prices[0];
        for(int i=1;i<n;i++)
        {
            int curr=prices[i]-mini;
            maxP=max(maxP,curr);
            mini=min(mini,prices[i]);
        }
        return maxP;
        
    }
};
